        "[('conv1', (96, 3, 11, 11)),\n",
        " ('conv2', (256, 48, 5, 5)),\n",
        " ('conv3', (384, 256, 3, 3)),\n",
        " ('conv4', (384, 192, 3, 3)),\n",
        " ('conv5', (256, 192, 3, 3)),\n",
        " ('fc6', (4096, 9216)),\n",
